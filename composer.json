{
    "name": "memnunal/papara-payment-gateway",
    "type": "library",
    "description": "The Laravel eCommerce Papara Payment Gateway",
    "keywords": [
        "memnunal",
        "bagisto",
        "payment",
        "ecommerce",
        "papara",
        "opensource"
    ],
    "license": "MIT",
    "homepage": "https://memnunal.com",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Hamed AGHAEIAN CHELGHAEI",
            "email": "aghaieyan@gmail.com",
            "homepage": "https://github.com/aghaeian",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/aghaeian/papara-payment-gateway",
        "issues": "https://github.com/aghaeian/papara-payment-gateway/issues",
        "email": "aghaieyan@gmail.com"
    },
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1 || ^8.2",
        "laravel/framework": "^8.0 || ^9.0 || ^10.0",
        "papara/papara-php": "^2.0,
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/aghaeian/papara-payment-gateway.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Webkul\\Papara\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkul\\Papara\\Providers\\PaparaServiceProvider"
            ],
            "aliases": {}
        }
    }
}
